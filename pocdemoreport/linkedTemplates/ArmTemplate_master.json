{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pocdemoreport"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"Snowflake1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake1'"
		},
		"is_ablob_covidreportings_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'is_ablob_covidreportings'"
		},
		"is_adl_covidreportinsadl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'is_adl_covidreportinsadl'"
		},
		"sinkcasesanddeaths_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sinkcasesanddeaths'"
		},
		"is_adl_covidreportinsadl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reportadl.dfs.core.windows.net/"
		},
		"ls_ecdc_covid19_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourcebaseURL}"
		},
		"sinkcasesanddeaths_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reportadl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pocdemoreport_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"Snowflake1_connectionString": {
						"value": "[parameters('Snowflake1_connectionString')]"
					},
					"is_ablob_covidreportings_connectionString": {
						"value": "[parameters('is_ablob_covidreportings_connectionString')]"
					},
					"is_adl_covidreportinsadl_accountKey": {
						"value": "[parameters('is_adl_covidreportinsadl_accountKey')]"
					},
					"sinkcasesanddeaths_accountKey": {
						"value": "[parameters('sinkcasesanddeaths_accountKey')]"
					},
					"is_adl_covidreportinsadl_properties_typeProperties_url": {
						"value": "[parameters('is_adl_covidreportinsadl_properties_typeProperties_url')]"
					},
					"ls_ecdc_covid19_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_covid19_properties_typeProperties_url')]"
					},
					"sinkcasesanddeaths_properties_typeProperties_url": {
						"value": "[parameters('sinkcasesanddeaths_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "pocdemoreport_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'pocdemoreport_ArmTemplate_0')]"
			]
		}
	]
}